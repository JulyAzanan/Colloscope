        <Rectangle HorizontalAlignment="Left" Height="980" Margin="366,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="26" Fill="AliceBlue"/>
        <StackPanel x:Name="stack" Margin="34,81,1178,81" RenderTransformOrigin="0.035,0.02"/>
        <StackPanel x:Name="stack_eleves1" Margin="434,91,811,71" RenderTransformOrigin="0.035,0.02"/>
        <StackPanel x:Name="stack_eleves2" Margin="700,91,534,71" RenderTransformOrigin="0.035,0.02"/>
